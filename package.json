{
  "name": "lifecycle",
  "version": "1.0.0",
  "cacheDirectories": [
    "node_modules",
    "vendor"
  ],
  "scripts": {
    "babel-node": "babel-node --extensions '.ts'",
    "dev": "LOG_LEVEL=debug ts-node -r tsconfig-paths/register --project tsconfig.server.json ws-server.ts | pino-pretty -c -t HH:MM -i pid,hostname,filename -o '{msg}'",
    "build": "next build && tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "start": "NEXT_MANUAL_SIG_HANDLE=true NODE_ENV=production node .next/ws-server.js",
    "run-prod": "port=5001 pnpm run start",
    "knex": "pnpm run knex",
    "test": "NODE_ENV=test jest --maxWorkers=75%",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "ts-check": "tsc --project tsconfig.json",
    "db:migrate": "NODE_OPTIONS='--loader ts-node/esm' knex migrate:latest",
    "db:rollback": "NODE_OPTIONS='--loader ts-node/esm' knex migrate:rollback",
    "db:seed": "knex seed:run",
    "prepare": "husky install",
    "generate:jsonschemas": "tsx ./scripts/generateSchemas.ts generatejson",
    "generate:yamls": "tsx ./scripts/generateSchemas.ts generateyaml",
    "generate:schemas": "pnpm run generate:jsonschemas && pnpm run generate:yamls"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.22.3",
    "@octokit/auth-app": "^6.0.2",
    "@octokit/core": "^5.0.2",
    "@octokit/webhooks": "^12.0.1",
    "aws-sdk": "^2.1004.0",
    "axios": "^0.30.0",
    "bcrypt": "^5.1.0",
    "bull": "^3.16.0",
    "commander": "^7.1.0",
    "connection-string": "^3.1.0",
    "dagre": "^0.8.5",
    "dd-trace": "^5.10.0",
    "dotenv": "^8.0.0",
    "fastly": "^7.0.1",
    "flatted": "^3.0.4",
    "haikunator": "^2.1.2",
    "hot-shots": "^10.0.0",
    "ioredis": "^4.27.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.2",
    "kubernetes-client": "^8.3.4",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.24.0",
    "mustache": "^4.1.0",
    "nanoid": "^3.1.31",
    "next": "^14.0.3",
    "object-hash": "^2.0.3",
    "objection": "^3.0.1",
    "p-queue": "^6.6.2",
    "pg": "^8.11.0",
    "pino": "^8.16.2",
    "pino-caller": "^3.4.0",
    "psl": "^1.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redlock": "^4.2.0",
    "seedrandom": "^3.0.5",
    "shelljs": "^0.8.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.19.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@octokit/webhooks-types": "^7.6.1",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^27.0.3",
    "@types/js-cookie": "^2.2.4",
    "@types/js-yaml": "^4.0.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.135",
    "@types/mustache": "^4.1.1",
    "@types/node": "^12.0.12",
    "@types/object-hash": "^1.3.1",
    "@types/psl": "^1.1.0",
    "@types/react": "^16.9.12",
    "@types/redlock": "^4.0.1",
    "@types/seedrandom": "^3.0.0",
    "@types/shelljs": "^0.8.5",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsdoc": "^39.7.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.0",
    "inquirer": "^12.1.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "mockdate": "^2.0.3",
    "only-allow": "^1.1.1",
    "pino-pretty": "^10.0.0",
    "prettier": "2.8.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.2",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run lint:fix",
      "prettier --write"
    ],
    "src/server/lib/yamlSchemas/**/*.ts": [
      "pnpm run generate:schemas",
      "git add src/server/lib/jsonschema/schemas/*.json",
      "git add docs/schema/yaml/*.yaml"
    ],
    "*.{ts,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts)$": "@swc/jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.ts"
    ],
    "testEnvironment": "jest-environment-node",
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "moduleNameMapper": {
      "^server/(.*)": "<rootDir>/src/server/$1",
      "^shared/(.*)": "<rootDir>/src/shared/$1",
      "^src/(.*)": "<rootDir>/src/$1",
      "^scripts/(.*)": "<rootDir>/scripts/$1",
      "^root/(.*)": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/server/**/*.ts",
      "<rootDir>/src/client/**/*.ts"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "next",
      "plugin:react/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "eslint-plugin-import",
      "eslint-plugin-react",
      "eslint-plugin-jsdoc",
      "eslint-plugin-prefer-arrow",
      "eslint-plugin-unicorn",
      "@typescript-eslint"
    ],
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "rules": {
          "no-undef": "off"
        }
      }
    ],
    "root": true
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "pnpm": {
    "overrides": {
      "@kubernetes/client-node": "^0.22.3"
    }
  }
}
